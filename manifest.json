{

	"name": "Whatsapp", 
	"author": "Author",
	"version": "1.0.0.0",
	"description": "Whatsapp",
	"default_locale": "en",

	"manifest_version": 3,
	"permissions": [
	    "storage",
		"alarms",
		"notifications"
	],
	"icons": {
		"16": "main/img/icon.png",
		"48": "main/img/icon.png",
		"128": "main/img/icon.png"
	},
    "host_permissions": [
    	"<all_urls>"
    ],
	"action": {
		"default_icon": "main/img/icon.png",
		"default_title": "Whatsapp",      
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js": [
			"main/js/jquery.js", 
			"main/js/custom.js"
		],
		"css": ["fix.css"],
		"all_frames": true,
		"run_at": "document_start"
	}]
}
